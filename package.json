{
  "name": "workspace",
  "version": "1.30.0",
  "license": "MIT",
  "scripts": {
    "ls2": "ls",
    "generate-schema": "pnpm nx run argon:generate-schema",
    "generate-client": "pnpm nx run argon:generate-client",
    "inner:argon:concat-schema": "cp $(find libs -name '*.prisma') apps/argon/prisma/schema",
    "inner:argon:db-push": "pnpm inner:argon:concat-schema && pnpm exec prisma db push",
    "inner:argon:generate-schema": "pnpm inner:argon:concat-schema && pnpm exec prisma migrate dev --name argon --schema apps/argon/prisma/schema",
    "inner:argon:generate-client": "pnpm inner:argon:concat-schema && pnpm exec prisma generate --generator client --schema apps/argon/prisma/schema",
    "inner:argon:deploy-schema": "pnpm exec prisma deploy"
  },
  "dependencies": {
    "@nx/workspace": "^20.0.6",
    "@prisma/client": "^5.21.1",
